{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "GDB Debugging",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceRoot}/translate.exe",
            "args": [
                /*"gradeIn.txt",
                "gradeOut.txt",
                "studentCode.txt",
                "courseCode.txt",*/
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": true,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enalbe pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "build all",
            "miDebuggerArgs": "C:\\TDM-GCC-64\\bin\\gdb.exe",
            "internalConsoleOptions": "openOnSessionStart"
        }
    ]
}